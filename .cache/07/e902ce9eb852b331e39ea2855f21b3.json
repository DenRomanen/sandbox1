teners();\n  }\n\n  getTotal() {\n    return this.products.reduce((acc, product) => acc + product.cost, 0);\n  }\n\n  addProduct(product) {\n    this.products.push(product);\n    this.update();\n    this.render();\n  }\n\n  makeOrder() {\n    console.log(\"Товары оформлены:\")\n    this.products.map(product => console.log(product.name));\n  }\n\n  removeListeners() {\n\n  }\n\n  addListeners() {\n    this.cart.querySelector(\"#makeOrderBtn\").addEventListener(\"click\", this.makeOrder.bind(this));\n  }\n}"},"lineCount":null}},"error":null,"hash":"b91b63d4474fcd198b7dd2594ba1d48e","cacheData":{"env":{}}}